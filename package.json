{
  "name": "multistore-electron",
  "version": "0.0.0",
  "author": "Mazino Ukah <ewomaukah@yahoo.com>",
  "description": "Multistore app (electron)",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "sideEffects": false,
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "multistore-electron",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "animate.css": "^3.6.1",
    "autotable": "^1.0.0",
    "axios": "^0.18.0",
    "base64-img": "^1.0.4",
    "basscss": "^8.0.2",
    "buefy": "^0.5.3",
    "chart.js": "^2.7.1",
    "debounce": "^1.1.0",
    "element-ui": "^2.3.3",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "html2canvas": "^1.0.0-alpha.12",
    "inherits": "^2.0.3",
    "jquery": "^3.2.1",
    "jspdf": "^1.3.5",
    "jspdf-autotable": "^2.3.2",
    "lodash": "^4.17.4",
    "material-icons": "^0.2.1",
    "moment": "^2.18.1",
    "node-sass": "^4.8.3",
    "number-words": "^1.0.6",
    "numeral": "^2.0.6",
    "papaparse": "^4.3.7",
    "pdfmake": "^0.1.36",
    "perfect-scrollbar": "^0.8.1",
    "pluralize": "^5.0.0",
    "rasterizehtml": "^1.3.0",
    "safe-buffer": "^5.1.1",
    "sass-loader": "^6.0.7",
    "serve-static": "^1.5.4",
    "sw-precache-webpack-plugin": "^0.11.5",
    "sweetalert": "^2.0.8",
    "vue": "^2.5.3",
    "vue-acl": "^2.2.3",
    "vue-avatar-component": "^1.2.4",
    "vue-axios": "^2.0.2",
    "vue-chart-js": "^1.4.1",
    "vue-chartjs": "^3.0.2",
    "vue-core-image-upload": "^2.4.5",
    "vue-electron": "^1.0.6",
    "vue-infinite-loading": "^2.2.2",
    "vue-js-popover": "^1.1.7",
    "vue-json-excel": "^0.2.5",
    "vue-lodash": "^2.0.0",
    "vue-momentjs": "^0.1.2",
    "vue-progressbar": "^0.7.2",
    "vue-router": "^2.5.3",
    "vue-scrollto": "^2.7.2",
    "vue-sweetalert2": "^1.0.7",
    "vue2-dropzone": "^3.0.0",
    "vuedraggable": "^2.16.0",
    "vuelidate": "^0.6.1",
    "vuex": "^2.3.1",
    "vuex-persistedstate": "^1.4.1"
  },
